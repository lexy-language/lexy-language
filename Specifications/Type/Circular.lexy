type NestedType
  number NumberValue
  string StringValue
  date DateValue
  boolean BooleanValue
  DeclaredType Custom

type DeclaredType
  number NumberValue
  string StringValue
  date DateValue
  boolean BooleanValue
  NestedType Nested

scenario ScenarioCircularReference
  function
    DeclaredType invalidType
  expectComponentErrors
    "Circular reference detected in: 'NestedType'"
    "Circular reference detected in: 'DeclaredType'"