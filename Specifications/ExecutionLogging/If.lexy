      Log "Parameters"
        Evil = false
      Log "number temp = 777"
        temp = 777
      Log "if Evil"
        Evil = false
      Log "else"
        Log "  temp = 888"
          temp = 888
      Log "Number = temp"
        temp = 888
        Number = 888
      Log "Results"
        Number = 888