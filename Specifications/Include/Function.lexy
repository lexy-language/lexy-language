include "Enum"
include "Table"

function IncludeFunction
  parameters
    number NumberValue
    IncludeEnum EnumValue
  results
    number NumberResult
    IncludeEnum EnumResult
    string StringResult

  NumberResult = NumberValue
  EnumResult = EnumValue
  StringResult = lookUp(IncludeTable, NumberValue, IncludeTable.SearchValue, IncludeTable.ResultString)