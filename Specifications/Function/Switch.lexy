function EnumSwitch
  Parameters
    SwitchValue Level
  Results
    string Number
  Code
    string temp = "999"
    switch Level
      case SwitchValue.Evil
        temp = "666"
      case SwitchValue.Holy
        temp = "777"
      default
        temp = "888"
    Number = temp

Scenario: EnumSwitchCaseHoly
  function EnumSwitch
  Parameters
    Level = SwitchValue.Holy
  Results
    Number = "777"

Scenario: EnumSwitchCaseEvil
  function EnumSwitch
  Parameters
    Level = SwitchValue.Evil
  Results
    Number = "666"

Scenario: EnumSwitchDefault
  function EnumSwitch
  Parameters
    Level = SwitchValue.Unknown
  Results
    Number = "888"




function EnumNestedSwitch
  Parameters
    boolean Evil
    SwitchValue Level
  Results
    string Number
  Code
    string temp = "999"
    switch Level
      case SwitchValue.Evil
        if Evil
          temp = "666"
        else
          temp = "555"
      case SwitchValue.Holy
        temp = "777"
      default
        temp = "888"
    Number = temp

Scenario: EnumNestedSwitchIfFalse
  function EnumNestedSwitch
  Parameters
    Level = SwitchValue.Evil
    Evil = false
  Results
    Number = "555"

Scenario: EnumNestedSwitchIfTrue
  function EnumNestedSwitch
  Parameters
    Level = SwitchValue.Evil
    Evil = true
  Results
    Number = "666"
