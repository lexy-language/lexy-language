type ComplexType
  number InnerNumber

type CustomType
  number NumberValue
  ComplexType Complex

scenario NestedParametersShouldBeAssignableAndReadable
  function
    parameters
      CustomType Result
    results
      number Value1
      number Value2
    Value1 = Result.NumberValue
    Value2 = Result.Complex.InnerNumber
  parameters
    Result.NumberValue = 9
    Result.Complex.InnerNumber = 8
  results
    Value1 = 9
    Value2 = 8