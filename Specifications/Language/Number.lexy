scenario InvalidNumberEndsWithLetter
  function
    results
      number Result
    Result = 123A
  expectErrors
    "Invalid number token character: 'A'"

scenario InvalidNumberInitializeString
  function
    parameters
      number Value = "1"
    results
      number Result
    Result = Value
  expectErrors
    "Invalid default value '1'. (type: 'number')"

scenario InvalidNumberInitializeBoolean
  function
    parameters
      number Value = true
    results
      number Result
    Result = Value
  expectErrors
    "Invalid default value 'true'. (type: 'number')"

scenario InvalidNumberInitializeDate
  function
    parameters
      number Value = d"2024-12-23T17:51:15"
    results
      number Result
    Result = Value
  expectErrors
    "Invalid default value '2024-12-23T17:51:15'. (type: 'number')"