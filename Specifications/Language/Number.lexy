scenario NumberIntAsParameterDefault
  function
    parameters
      number Value = 123
    results
      number Result
    Result = Value
  results
    Result = 123

scenario NumberIntAsParameter
  function
    parameters
      number Value = 123
    results
      number Result
    Result = Value
  parameters
    Value = 456
  results
    Result = 456

scenario NumberDecimalAsParameter
  function
    parameters
      number Value = 123.45
    results
      number Result
    Result = Value
  results
    Result = 123.45

scenario NumberIntInCode
