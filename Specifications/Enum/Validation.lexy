scenario InvalidEnumNegativeValue
  enum InvalidEnumNegativeValueEnum
    First = 0
    Second = -1
  expectErrors
    "Validation.lexy(4, 15): ERROR - Invalid number of tokens: 4. Should be 1 or 3."

scenario InvalidEnumDecimalValue
  enum InvalidEnumDecimalValueEnum
    First = 0
    Second = 1.1
  expectErrors
    "Enum member value should not be decimal: 1.1"

scenario InvalidEnumDuplicatedMember
  enum InvalidVEnumDuplicatedMemberEnum
    First = 0
    First = 1
  expectErrors
    "Enum member name should be unique. Duplicate name: 'First'"

enum ValidEnum
  First
  Second
  Third

scenario InvalidEnumInitializeNumber
  function
    parameters
      ValidEnum Value = 1
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value '1'. (type: 'ValidEnum')"

scenario InvalidEnumInitializeString
  function
    parameters
      ValidEnum Value = "a"
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value 'a'. (type: 'ValidEnum')"

scenario InvalidEnumInitializeBoolean
  function
    parameters
      ValidEnum Value = true
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value 'true'. (type: 'ValidEnum')"

scenario InvalidEnumInitializeDate
  function
    parameters
      ValidEnum Value = d"2024-12-23T17:51:15"
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value '2024-12-23T17:51:15'. (type: 'ValidEnum')"

scenario InvalidEnumInitializeNoMember
  function
    parameters
      ValidEnum Value = ValidEnum
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value 'ValidEnum'. (type: 'ValidEnum')"

scenario InvalidEnumInitializeUnknownMember
  function
    parameters
      ValidEnum Value = ValidEnum.Fourth
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value 'ValidEnum.Fourth'. Invalid member. (type: 'ValidEnum')"
    "Invalid identifier: 'ValidEnum.Fourth'"

scenario InvalidEnumInitializeInvalidEnumType
  function
    parameters
      ValidEnum Value = InvalidEnum.Fourth
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value 'InvalidEnum.Fourth'. Invalid enum type. (type: 'ValidEnum')"
    "Invalid default value 'InvalidEnum.Fourth'. Invalid member. (type: 'ValidEnum')"
    "Invalid identifier: 'InvalidEnum.Fourth'"

scenario InvalidEnumInitialize
  function
    parameters
      ValidEnum Value = ValidEnum.Third.First
    results
      ValidEnum Result
    Result = Value
  expectErrors
    "Invalid default value 'ValidEnum.Third.First'. (type: 'ValidEnum')"
    "Invalid identifier: 'ValidEnum.Third.First'"
