  var removeFirst = values.Dequeue()
  var first5 = values.Take(5)
  var returns5and6 = values.FromTake(5, 2)
  var returns567 = values.FromTo(5, 7)
  var returns567 = values.Length
  var returns567 = values.Contains(10)

  //Dictionary
  var values = (number, string)
  var value = values[5]
  values.add(5, "123")
  var lastWithoutRemoving = values.Peek()
  var removeFirst = values.Dequeue()
  var first5 = values.Take(5)
