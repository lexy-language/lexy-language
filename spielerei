YearMonth yearMonths = {Message: "", Year: Date.Now().Year(), Amount: 0}
(string, YearMonth) yearMonths = (
    "jan": {Month: 0, Year: Date.Now().Year(), Amount: 0},
    "feb": {Month: 1, Year: Date.Now().Year(), Amount: 0},
    "mar": {Month: 2, Year: Date.Now().Year(), Amount: 0},
  )

type YearMonth
  number Month = 0
  number Year
  number amount

  function (string Message, boolean Success) NextMonth (number Amount)
    
    Assert.True(amount > 0, "Amount should be greater as 0 (is {amount})")
    
    if Index == 11
      Year += 1
      Index = Index + 1
    else
      Index = Index + 1

    Amount = Amount + amount
    
    Message = "total: " + Amount
    Success = true
